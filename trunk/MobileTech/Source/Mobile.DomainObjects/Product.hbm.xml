<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="RMS.DomainObjects" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Mobile.DomainObjects.Product, Mobile.DomainObjects" table="Product" lazy="true" >
    <id name="ID" type="Int32" column="ID">
      <generator class="identity" />
    </id>
    <property name="ProductName" column="ProductName" not-null="true" type="string" insert="true" update="true" />
    <property name="ProductPrice" column="ProductPrice" not-null="true" type="decimal" insert="true" update="true" />
    <property name="ProductImage" column="ProductImage" not-null="true" type="string" insert="true" update="true" />
  </class>
</hibernate-mapping>