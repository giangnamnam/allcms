<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="RMS.DomainObjects" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Mobile.DomainObjects.Company, Mobile.DomainObjects" table="Company" lazy="true" >
